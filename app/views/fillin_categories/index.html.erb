<p id="notice"><%= notice %></p>

<h1>Fillin Categories</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fillin_categories.each do |fillin_category| %>
      <tr>
        <td><%= fillin_category.name %></td>
        <td><%= link_to 'Show', fillin_category %></td>
        <td><%= link_to 'Edit', edit_fillin_category_path(fillin_category) %></td>
        <td><%= link_to 'Destroy', fillin_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fillin Category', new_fillin_category_path %>
